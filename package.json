{ 
  "scripts": {
    "start": "sudo $(which node) $(which forever) start -a -l forever.log -o stdout.log -e stderr.log bot.js",
    "stop": "sudo $(which node) $(which forever) stop bot.js",
    "restart": "npm stop && npm start",
    "dev": "node ./bot.js"
  },
  "dependencies": {
    "@discordjs/builders": "^1.0.0",
    "@discordjs/rest": "^1.0.0",
    "discord-api-types": "^0.36.2",
    "discord.js": "^13.0.1",
    "ethers": "^5.6.9",
    "forever": "^4.0.3",
    "googleapis": "^39.2.0",
    "moment": "^2.29.4",
    "mongoose": "^6.4.5"
  }
}
